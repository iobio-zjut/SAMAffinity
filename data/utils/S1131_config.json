{
    "common": {
        "csv_file": "./data/S1131.csv",
        "fasta_dir": "./data/S1131_fasta",
        "suffix_list": [
            "a", "a_m", "b", "b_m"
        ]
    },

    "output_dirs": {
        "esm2": "./data/S1131_esm2_emb",
        "prott5": "./data/S1131_prott5",
        "pssm": "./data/S1131_PSSM"
    },

    "script_paths": {
        "esm2": "./data/utils/ESM2_generate.py",
        "prott5": "./data/utils/ProtT5_generate.py",
        "pssm": "./data/utils/PSSM_generate.py"
    },

    "esm2_specific": {
        "esm_model_path": "./data/esm2_t33_650M_UR50D.pt",
        "nogpu": false
    },

    "prott5_specific": {
        "model_path": "./data/prott5",
        "per_protein": 0,
        "half": 0,
        "is_3Di": 0
    },
    "pssm_specific": {
        "psiblast_path": "./data/ncbi-blast-2.12.0+/bin/psiblast",
        "db_path": "./data/ncbi-blast-2.12.0+/bin/swissprot"
    }

}
